<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="csc207phase2.gamecentre">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="csc207phase2.gamecentre.GameCentreActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="csc207phase2.gamecentre.SlidingTilesGameActivity"></activity>
        <activity android:name="csc207phase2.gamecentre.SlidingTilesActivity"></activity>
        <activity android:name="csc207phase2.gamecentre.SignInActivity"></activity>
        <activity android:name="csc207phase2.gamecentre.SignUpActivity"></activity>
        <activity android:name="csc207phase2.gamecentre.GameSelectActivity"></activity>git
        <activity android:name="csc207phase2.gamecentre.OptionsActivity"></activity>
        <activity android:name="csc207phase2.gamecentre.ProfileActivity"></activity>
        <activity android:name="csc207phase2.gamecentre.ChooseComplexity"></activity>
        <activity android:name="csc207phase2.gamecentre.MemoryGameActivity"></activity>
        <activity android:name="csc207phase2.gamecentre.MemoryGameStartActivity"></activity>
        <activity android:name="csc207phase2.gamecentre.MemoryChooseComplexity"></activity>
        <activity android:name="csc207phase2.gamecentre.MinesweeperMenuActivity"></activity>
        <activity android:name="csc207phase2.gamecentre.MinesweeperComplexity"></activity>
        <activity android:name="csc207phase2.gamecentre.MinesweeperGameActivity"></activity>
        <activity android:name="csc207phase2.gamecentre.ScoreActivity"></activity>
    </application>
</manifest>
